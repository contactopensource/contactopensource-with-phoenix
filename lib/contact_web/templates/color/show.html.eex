<h1>Show Color</h1>

<ul>

  <li>
    <strong>Updated at timestamp utc:</strong>
    <%= @color.updated_at_timestamp_utc %>
  </li>

  <li>
    <strong>Updated at clock count:</strong>
    <%= @color.updated_at_clock_count %>
  </li>

  <li>
    <strong>Updated by uri:</strong>
    <%= @color.updated_by_uri %>
  </li>

  <li>
    <strong>Tenant uri:</strong>
    <%= @color.tenant_uri %>
  </li>

  <li>
    <strong>Type uri:</strong>
    <%= @color.type_uri %>
  </li>

  <li>
    <strong>State uri:</strong>
    <%= @color.state_uri %>
  </li>

  <li>
    <strong>Rgb unit interval r:</strong>
    <%= @color.rgb_unit_interval_r %>
  </li>

  <li>
    <strong>Rgb unit interval g:</strong>
    <%= @color.rgb_unit_interval_g %>
  </li>

  <li>
    <strong>Rgb unit interval b:</strong>
    <%= @color.rgb_unit_interval_b %>
  </li>

  <li>
    <strong>Cymk unit interval c:</strong>
    <%= @color.cymk_unit_interval_c %>
  </li>

  <li>
    <strong>Cymk unit interval y:</strong>
    <%= @color.cymk_unit_interval_y %>
  </li>

  <li>
    <strong>Cymk unit interval m:</strong>
    <%= @color.cymk_unit_interval_m %>
