  <li>
    <strong>Cymk unit interval k:</strong>
    <%= @color.cymk_unit_interval_k %>
  </li>

  <li>
    <strong>Hex:</strong>
    <%= @color.hex %>
  </li>

  <li>
    <strong>Html color name:</strong>
    <%= @color.html_color_name %>
  </li>

  <li>
    <strong>X11 color name:</strong>
    <%= @color.x11_color_name %>
  </li>

  <li>
    <strong>Color naming system name:</strong>
    <%= @color.color_naming_system_name %>
  </li>

  <li>
    <strong>Pantone matching system name:</strong>
    <%= @color.pantone_matching_system_name %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.color_path(@conn, :edit, @color) %></span>
<span><%= link "Back", to: Routes.color_path(@conn, :index) %></span>
